<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.jamesou.dailycost.mapper.CategoryMapper">
    <select id="selectCategoryByState" resultType="com.jamesou.dailycost.bean.Category">
        select * from tb_category
        where categoryState = #{categoryState}
    </select>
</mapper>